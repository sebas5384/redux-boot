{
  "name": "redux-boot",
  "version": "1.1.1",
  "description": "Minimal Framework using Redux to develop modularized universal.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build-es && npm run build-umd",
    "build-es": "rm -Rf lib && BABEL_ENV=es babel ./src -d lib",
    "build-umd": "rm -f build/index.js* && webpack",
    "pretest": "rm -f build/test.js* && webpack --config ./webpack.config.test.js",
    "test": "tape build/test.js | tap-spec",
    "prepublish": "npm run build"
  },
  "directories": {
    "lib": "src/lib"
  },
  "author": "",
  "license": "MIT",
  "repository": "choko-org/redux-boot",
  "peerDependencies": {
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-promise": "^0.5.3"
  },
  "devDependencies": {
    "axios": "^0.11.0",
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "flux-standard-action": "^0.6.1",
    "gitbook-cli": "^2.1.3",
    "json-loader": "^0.5.4",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-promise": "^0.5.3",
    "symbol-observable": "^0.2.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "webpack": "^1.12.14"
  }
}
